{
  "manifest_version": 3,
  "name": "VideoCollect",
  "description": "视频采集.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.dandanzan.com/*",
        "https://www.dandanzan.club/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentscript.js",
        "inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "inject.js"
      ],
      "matches": [
        "https://www.dandanzan.com/*",
        "https://www.dandanzan.club/*"
      ]
    }
  ]
}